     <UrlRewriteRule enabled="true">
    <add url="~/Industrial-Ethernet-Networking/Media-Converters/IEN_Media_Converters.mx.htm" mappedUrl="/Product/ProductCategory.aspx?category=MstrCATE_EAPRO_RSCOMM" />
